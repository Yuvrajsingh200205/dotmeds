        /* General styles */
body {
    font-family: Arial, sans-serif;
    margin: 0px;
    padding: 0px;
    background-color: #d2d0d0;
    overflow-x: hidden; 
}

/* Header styles */


header.page-header {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    background-color: #09bd93; 
    z-index: 1000; 
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
    margin-bottom: 50px;
    width: 97vw;
}
.page-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 10px 20px;
    background-color: #09bd93;
    
   
}

/* Logo styles */
.page_logo  {
    height: 40px;
    width: auto;
     color: #0e1a1c;
    padding: 5px;
    border-radius: 5px;
    font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
}
.page_logo i {
    padding: 5px;
 font-size: 30px;

}

/* Search bar styles */
.page_search form {
    display: flex;
    align-items: center;
}

.page_search input[type="text"] {
    padding: 5px;
    font-size: 13px;
    border: 1px solid #ccc;
    border-radius: 8px 0 0 8px;
    outline: none;
    width: 500px;
    height: 30px;
}

.page_search button {
    padding: 5px 10px;
    font-size: 14px;
    border: 1px solid #ccc;
    border-left: none;
    border-radius: 0 8px 8px 0;
    background-color: #7cfffe;
    /* color: white; */
    height: 42px;
    cursor: pointer;
}

.page_search button:hover {
    background-color: #00ffbf;
}

/* Header navigation links styles */
.page-header div {
    display: flex;
    align-items: center;
}

.page-header div a {
    text-decoration: none;
    color: #ffffff;
    margin-left: 5px;
    font-size: 16px;
}

.page-header div i {
    margin-right: 5px;
    color: #ffffff;
}
 
#LoginButton{
    cursor: pointer;
    margin-right: 20px ;
    color: white;
}
a{
    color: black;
    text-decoration: none;
}

/* Specific sections styles */
.Upload, .offer, .cart, .sign_in {
    margin-left: 20px;
}

.Upload a, .offer a, .cart a, .sign_in a {
    display: flex;
    align-items: center;
}

/* sliding style adding the css */
/* slidingl styles */
.sliding {
    position: relative;
    width: 100%;
    overflow: hidden;
}

.sliding-images {
    padding-top: 1.22rem;
    display: flex;
    transition: transform 0.5s ease-in-out;
}

.sliding-images img {
    width: 100%;
    flex: 0 0 100%;
}

.sliding-btn {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background-color: rgba(0, 0, 0, 0.5);
    color: white;
    border: none;
    padding: 10px;
    cursor: pointer;
    z-index: 1;
}

.sliding-btn.prev {
    left: 0;
}

.sliding-btn.next {
    right: 0;
}




/* Adds-box styles */
.adds-box {
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
    margin: 25px 0;
}

.adds-box .box-1, .adds-box .box-2 {
    flex: 1 1 30%;
    box-sizing: border-box;
    padding: 10px;
    margin: 10px;
    background-color: #f0f0f0;
    border-radius: 8px;
    text-align: center;
    display: flex;
    align-items: center; 
}

.adds-box .box-1 img, .adds-box .box-2 img {
    width: 20%; 
    height: auto;
    border-radius: 50px;
    margin-right: 10px; /* Space between image and text */
}

.adds-box .box-1 .text-content, .adds-box .box-2 .text-content {
    flex: 1;
    text-align: left;
      flex: 1; /* Align text to left */
}

.adds-box h2 {
    margin: 10px 0;
    font-size: 26px;
}

.adds-box h4 {
    margin:  0;
    font-size: 16px;
    color: #09bd93;
}


/* second box  */
